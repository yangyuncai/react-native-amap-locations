{
  "presets": [
    [
      "env",
      {
        "loose": true,
        "targets": {
          "browsers": ["last 2 versions", "safari >= 8"]
        }
      }
    ],
    "stage-3"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    "transform-class-properties"
  ]
}
